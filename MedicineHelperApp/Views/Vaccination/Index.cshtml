@model List<VaccinationModel>

@{
    ViewData["Title"] = "Vaccination";
}

<h1 -align>Vaccination</h1>


    @if (Model.Any())
    {
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Vaccine</th>
                    <th scope="col">Vaccination date</th>
                    <th scope="col">User</th>
                    <th scope="col">Doctor</th>
                    <th scope="col">Status</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                await Html.RenderPartialAsync("VisitsList", item);
            }
        </table>
        <a asp-action="Add" asp-controller="Vaccination">Add vaccination</a>

    }
